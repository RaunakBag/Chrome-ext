{
    "manifest_version": 2,

    "name": "Raunak's Favourite Song",
    "description": "Autoplay's Raunak's favourite song on Youtube",
    "version": "1.0.0",

    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    

    "permissions":[
        "tabs"
    ],
    "background": {
        "scripts": ["main.js"],
        "persistent": true
    }
}